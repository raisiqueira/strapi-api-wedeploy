{
  "id": "strapi",
  "image": "strapi/strapi",
  "volume": "/usr/src/api/strapi-app",
  "volume": "/usr/src/api/strapi-app/node_modules",
  "env": {
    "DATABASE_CLIENT": "mongo",
    "DATABASE_HOST": "db",
    "DATABASE_PORT": "27017",
    "DATABASE_NAME": "strapi",
    "APP_NAME": "strapi-app"
  },
  "zeroDowntime": true
}
